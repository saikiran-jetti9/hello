{
  "name": "beejav2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run && vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prepare": "cd .. && husky install [beeja-web]/.husky",
    "fullLint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fullFormat": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "fullScan": "npm run fullFormat && npm run fullLint"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.5.0",
    "buffer": "^6.0.3",
    "dotenv": "^16.3.1",
    "html2canvas": "^1.4.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "react-pdf": "^7.7.1",
    "react-router-dom": "^6.17.0",
    "sonner": "^1.4.3",
    "styled-components": "^6.1.0",
    "web-kit-components": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "prettier": "^3.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.5.0",
    "vite-plugin-react": "^4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run format && git add ."
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "npm run format",
      "npm run lint",
      "prettier --write",
      "eslint --fix",
      "git add ."
    ]
  }
}
